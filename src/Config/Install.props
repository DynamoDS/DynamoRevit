<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--ImportGroup Label="PropertySheets" Condition=" Exists('$(SolutionDir)Config/user_local.props') ">
    <Import Project="$(SolutionDir)Config/user_local.props" />
  </ImportGroup-->
  <PropertyGroup>
    <RevitVersionNumber Condition=" '$(RevitVersionNumber)' == '' ">2015</RevitVersionNumber>
	<REVIT_VERSION>Revit_$(RevitVersionNumber)</REVIT_VERSION>
	<DYNAMOTESTAPI Condition=" '$(DYNAMOTESTAPI)' == '' ">$(SolutionDir)..\..\Dynamo\bin\AnyCPU\$(Configuration)</DYNAMOTESTAPI>
    <DYNAMOAPI  Condition=" '$(DYNAMOAPI)' == '' ">$(SolutionDir)..\..\Dynamo\bin\AnyCPU\$(Configuration)</DYNAMOAPI>
	<OutputPath Condition=" '$(OutputPath)' == '' ">$(SolutionDir)..\bin\$(Platform)\$(Configuration)\$(REVIT_VERSION)</OutputPath>
	<TestOutputPath Condition=" '$(TestOutputPath)' == '' ">$(OutputPath)</TestOutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|x86'">
    <VisualStudioVersion Condition="'$(VisualStudioVersion)' == ''">14.0</VisualStudioVersion>
    <InstallOutputPath Condition=" '$(InstallOutputPath)' == '' ">$(MSBuildProjectDirectory)\bin\$(Platform)\$(Configuration)</InstallOutputPath>
    <DYNAMO_REVIT_BASE_PATH  Condition=" '$(DYNAMO_REVIT_BASE_PATH)' == '' ">$(OutputPath)..\..\..\..\..</DYNAMO_REVIT_BASE_PATH>
    <DYNAMO_HARVEST_PATH Condition=" '$(DYNAMO_HARVEST_PATH)' == '' ">$(MSBuildProjectDirectory)\harvest\$(REVIT_VERSION)</DYNAMO_HARVEST_PATH>
	<DYNAMO_RELEASE_PATH Condition=" '$(DYNAMO_RELEASE_PATH)' == '' ">$(MSBuildProjectDirectory)\harvest</DYNAMO_RELEASE_PATH>
    <DYNAMO_MIGRATION_NODES_PATH Condition=" '$(DYNAMO_MIGRATION_NODES_PATH)' == '' ">$(DYNAMO_REVIT_BASE_PATH)\doc\distrib\migration_nodes</DYNAMO_MIGRATION_NODES_PATH>
    <DYNAMO_SAMPLES_PATH Condition=" '$(DYNAMO_SAMPLES_PATH)' == '' ">$(DYNAMO_REVIT_BASE_PATH)\doc\distrib\Samples</DYNAMO_SAMPLES_PATH>
    <DYNAMO_REVIT_SIGN_TOOLS Condition=" '$(DYNAMO_REVIT_SIGN_TOOLS)' == '' ">$(MSBuildProjectDirectory)\sign</DYNAMO_REVIT_SIGN_TOOLS>
    <BaseIntermediateOutputPath>$(InstallOutputPath)\int\</BaseIntermediateOutputPath>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|x86' ">
    <VisualStudioVersion Condition="'$(VisualStudioVersion)' == ''">14.0</VisualStudioVersion>
    <InstallOutputPath Condition=" '$(InstallOutputPath)' == '' ">$(MSBuildProjectDirectory)\bin\$(Platform)\$(Configuration)</InstallOutputPath>
    <DYNAMO_REVIT_BASE_PATH  Condition=" '$(DYNAMO_REVIT_BASE_PATH)' == '' ">$(OutputPath)..\..\..\..\..</DYNAMO_REVIT_BASE_PATH>
    <DYNAMO_HARVEST_PATH Condition=" '$(DYNAMO_HARVEST_PATH)' == '' ">$(MSBuildProjectDirectory)\harvest\$(REVIT_VERSION)</DYNAMO_HARVEST_PATH>
	<DYNAMO_RELEASE_PATH Condition=" '$(DYNAMO_RELEASE_PATH)' == '' ">$(MSBuildProjectDirectory)\harvest</DYNAMO_RELEASE_PATH>
    <DYNAMO_MIGRATION_NODES_PATH Condition=" '$(DYNAMO_MIGRATION_NODES_PATH)' == '' ">$(DYNAMO_REVIT_BASE_PATH)\doc\distrib\migration_nodes</DYNAMO_MIGRATION_NODES_PATH>
    <DYNAMO_SAMPLES_PATH Condition=" '$(DYNAMO_SAMPLES_PATH)' == '' ">$(DYNAMO_REVIT_BASE_PATH)\doc\distrib\Samples</DYNAMO_SAMPLES_PATH>
    <DYNAMO_REVIT_SIGN_TOOLS Condition=" '$(DYNAMO_REVIT_SIGN_TOOLS)' == '' ">$(MSBuildProjectDirectory)\sign</DYNAMO_REVIT_SIGN_TOOLS>
    <BaseIntermediateOutputPath>$(InstallOutputPath)\int\</BaseIntermediateOutputPath>
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <DefineConstants>TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
</Project>